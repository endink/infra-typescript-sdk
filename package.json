{
  "name": "infra-sdk-core",
  "version": "1.1.0",
  "description": "A type script sdk for infra java spring server.",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --open development",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prettier": "prettier -c --write \"src/*\""
  },
  "keywords": [
    "npm",
    "plugin",
    "typescript"
  ],
  "author": "Anders Xiao",
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endink/infra-typescript-sdk.git"
  },
  "license": "Apache 2",
  "bugs": {
    "url": "https://github.com/endink/infra-typescript-sdk/issues"
  },
  "homepage": "https://github.com/endink/infra-typescript-sdk",
  "devDependencies": {
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/node": "^12.6.8",
    "@types/webpack": "^4.32.1",
    "@types/ali-oss": "^6.0.4",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.16",
    "ts-node": "^8.3.0",
    "pre-commit": "^1.2.2",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.3",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "moment": "^2.24.0",
    "umi-request": "^1.2.8"
  }
